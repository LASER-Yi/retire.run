{
  "name": "retire.run",
  "version": "0.1.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ci": "vite build -m development",
    "check": "eslint --ext .ts,.tsx src",
    "check:ts": "tsc --noEmit --incremental false",
    "check:fmt": "prettier -c .",
    "test": "exit 0",
