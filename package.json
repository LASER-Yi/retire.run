  "private": true,
  "dependencies": {
    "@mantine/core": "^4",
    "@mantine/dates": "^4",
    "@mantine/hooks": "^4",
    "i18next": "^21.6",
    "react": "latest",
    "react-dom": "latest",
    "react-i18next": "^11.16",
    "react-icons": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@vitejs/plugin-react": "latest",
    "eslint": "latest",
    "eslint-config-react-app": "latest",
    "eslint-plugin-react-hooks": "latest",
    "husky": "latest",
    "prettier": "latest",
    "prettier-plugin-organize-imports": "latest",
    "pretty-quick": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-checker": "latest",
    "vite-plugin-pwa": "latest",
    "worker-timers": "^7.0.45"
  }
